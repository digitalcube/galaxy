{
  "root": "libs/ui-marketing",
  "sourceRoot": "libs/ui-marketing/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/ui-marketing/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/ui-marketing"
      ],
      "options": {
        "jestConfig": "libs/ui-marketing/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build-tailwind": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "postcss tailwind.css -o public/tailwind.css",
        "cwd": "libs/ui-marketing"
      }
    },
    "watch-tailwind": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/ui-marketing",
        "commands": [
          "watch 'nx run ui-marketing:build-tailwind' src"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "config": {
          "configFolder": "libs/ui-marketing/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "dist/storybook/ui-marketing",
        "config": {
          "configFolder": "libs/ui-marketing/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}